package com.java1234.entity;

import java.math.BigDecimal;
import java.util.Date;


public class Goods {
	
	private Integer id;
	private String title;
	private BigDecimal price;
	private String spec;//规格
	private Integer kucun;//库存
	private Integer sales;//销量
	private String content;//详情   由富文本编辑器提供
	private String contentNoTag;
	private Integer isUse;//是否上架此商品
	private String showImg;//展示图片
	private Integer order_;
	
	
	//属于哪个分类
	private Integer fenleiId;
	private FenLei fenlei;
	
	private Integer createUserId;
	private User createUser;
	private Date createDateTime;
	
	
	//在什么位置显示   1推荐商品显示 
	private Integer pos;
	
	
	
	
	public Integer getPos() {
		return pos;
	}
	public void setPos(Integer pos) {
		this.pos = pos;
	}
	public Integer getOrder_() {
		return order_;
	}
	public void setOrder_(Integer order_) {
		this.order_ = order_;
	}
	public String getShowImg() {
		return showImg;
	}
	public void setShowImg(String showImg) {
		this.showImg = showImg;
	}
	public Integer getIsUse() {
		return isUse;
	}
	public void setIsUse(Integer isUse) {
		this.isUse = isUse;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getContentNoTag() {
		return contentNoTag;
	}
	public void setContentNoTag(String contentNoTag) {
		this.contentNoTag = contentNoTag;
